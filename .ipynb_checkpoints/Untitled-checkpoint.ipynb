    "import it_core_news_sm \n",
    "\n",
    "lemmatizer=it_core_news_sm.load()\n",
    "\n",
    "doc = lemmatizer('Fabio frattin')\n",
    "for t in doc:\n",
    "    print('Token:',t,' is a ', t.pos_)\n",
    "    if (t.pos_ in {'ADP','PROPN','PUNCT','DET','NOUN'}):\n",
    "        print('trovato')\n"
